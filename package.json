{
  "name": "esnext-quickstart",
  "version": "1.0.0",
  "description": "ESNext quickstart",
  "engines": {
    "node": ">=0.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nkbt/esnext-quickstart.git"
  },
  "author": "Nik Butenko <nik@butenko.me> (http://butenko.me/)",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:clean -s && npm run build:example -s",
    "build:clean": "rm -rf example",
    "build:example": "webpack",
    "start": "NODE_ENV=development webpack-dev-server",
    "lint": "eslint .",
    "karma": "karma start ./karma.conf.js",
    "test": "npm run karma -s --  --single-run --reporters nyan",
    "test:dev": "npm run karma -s -- --no-single-run --auto-watch --reporters nyan",
    "test:cov": "COVERAGE=1 npm run karma -s -- --single-run --reporters coverage",
    "check": "npm run lint -s && npm run test -s && npm run build -s"
  },
  "dependencies": {
    "babel-core": "^5.8.33",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.21.0",
    "eslint": "^1.7.3",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.6.3",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.2",
    "inject-loader": "^2.0.1",
    "isparta-loader": "^1.0.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.14",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.3.8",
    "karma-nyan-reporter": "0.2.2",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.10.1",
    "node-libs-browser": "^0.5.3",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.18",
    "react": "^0.14.1",
    "react-addons-test-utils": "^0.14.1",
    "react-dom": "^0.14.1",
    "react-hot-loader": "^1.3.0",
    "react-jasmine-matchers": "^2.0.0",
    "sorted-object": "^1.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
